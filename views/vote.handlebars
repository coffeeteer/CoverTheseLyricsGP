<h1 class="vote-title">Vote here</h1>

	<div class="container vote-container" id="vote-page-container">
			<div class="row">
        
		  {{#each Submissions}}
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 videobox">

<!-- 						<div class=> -->
					<iframe width="350" height="250" src="{{this.entry}}" frameborder="0" allowfullscreen></iframe>
					<span class="col-lg-2 col-md-6 col-sm-12 col-xs-12 vote-btn">
<!-- 							<span class="col-xs-6 col-md-4 vote-btn"> -->
					   <button type="button" class="btn btn-primary btn-md votebutton" name="vote" id="{{this.id}}">Vote For Me!</button>
					</span>
<!-- 						</div> -->
		
      </div>
		{{/each}}

			</div>
		</div>

 
<!-- added remodal jw 8/30 -->

<!--bootstrap modal -->
              


<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h1 class="modal-title" id="myModalLabel">Thank you for your vote.</h1>
      </div>
      <div class="modal-body">

      <p>
                  You have 5 votes total per day. Use them wisely!
                  Don't forget to tell your friends & family to vote for you!</p>
  
      </div>
      <div class="modal-footer">
        <button type="button" class="btn modalconfirm  btn-primary btn-lg" data-dismiss="modal">OK</button>
       
      </div>
    </div>
  </div>
</div>


<!-- added countdown clock -->

<div id="del-countdown">
<h2 class="timer-h2">LOADING....NEW CONTEST!!!</h2>
<div id="clock"></div>
<div id="units">
<span>Days</span>
<span>Hours</span>
<span>Minutes</span>
<span>Seconds</span>
</div>
</div>


</div>

